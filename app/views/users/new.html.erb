<h1>Sign Up</h1>

<%= form_for :user do |f| %>
  <%= f.label :username %> <br>
  <%= f.text_field :username %> <br>

  <%= f.label :email %> <br>
  <%= f.email_field :email %> <br>

  <br>

  <%= f.label :password %> <br>
  <%= f.password_field :password %> <br>

  <%= f.label :confirm_pass, "Confirm Password" %> <br>
  <%= f.password_field :confirm_pass %> <br>

  <br>

  <%= f.submit 'Create User' %>
<% end %>

